{
  "name": "react-lds-build",
  "private": true,
  "version": "0.1.0",
  "description": "Salesforce Lightning Design System React Components",
  "keywords": [
    "react",
    "react-component",
    "salesforce lightning design system",
    "lightning design system",
    "slds",
    "react-lds"
  ],
  "main": "./src/index.js",
  "scripts": {
    "test": "jest --silent",
    "test-watch": "jest --watch=all",
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel ./src --ignore *.spec.js --out-dir ./build",
    "build:copy-files": "babel-node ./copy-files.js",
    "prebuild": "rm -rf ./build",
    "lint": "eslint src"
  },
  "contributors": [
    "Konstantin Krauss <konstantin@propertybase.com> (http://www.propertybase.com)",
    "Christian Schultheiss <christian@propertybase.com> (http://www.propertybase.com)",
    "Ben Bieker <ben@propertybase.com> (http://www.propertybase.com)",
    "Philipp Frank <philipp@propertybase.com> (http://www.propertybase.com)",
    "Felix Sp√∂ttel <felix@propertybase.com> (http://www.propertybase.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/propertybase/react-lds.git"
  },
  "bugs": {
    "url": "https://github.com/propertybase/react-lds/issues"
  },
  "homepage": "https://github.com/propertybase/react-lds#readme",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "react-click-outside": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-jest": "12.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "doctrine": "1.2.2",
    "enzyme": "2.3.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-import-resolver-webpack": "0.3.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "5.1.1",
    "fs-extra": "^0.30.0",
    "highlight.js": "9.4.0",
    "jest-cli": "12.1.1",
    "json-loader": "0.5.4",
    "marked": "0.3.5",
    "node-sass": "3.7.0",
    "raw-loader": "0.5.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "15.1.0",
    "react-docgen": "2.8.2",
    "react-dom": "^15.0.0",
    "recast": "0.11.6",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.12.6"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
